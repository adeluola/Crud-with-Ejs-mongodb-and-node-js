<% for(var i = 0;i < user.length;i++){%>

<tr>
    <td><%= i+1%></td>
    <td><%= user[i].name %></td>
    <td><%= user[i].email %></td>
    <td><%= user[i].status %></td>
    <td><%= user[i].gender %></td>
    <td>
        <a href="/update_user?id=<%= user[i]._id%>" class="btn border-shadow update">
        <span class="text-gradient"><i class="fas fa-pencil-alt" aria-hidden="true"></i></span>
        </a>
        <a href="#" class="btn border-shadow delete" data-id=<%= user[i]._id%>>
            <span class="tex-gradient"><i class="fa fa-times" aria-hidden="true"></i>
            </span>
        </a>
    </td>
</tr>
<%}%>